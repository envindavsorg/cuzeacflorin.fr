  "scripts": {
    "dev": "next dev -p 1408 --turbopack",
    "dev:capture": "ENV_TYPE=capture next dev -p 1409 --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "preview": "next build --turbopack && next start -p 1408",
    "lint": "biome check .",
    "lint:fix": "biome format . --write",
    "check-types": "tsc --noEmit --pretty",
    "check-deps": "npx depcheck",
    "upgrade-deps": "npx taze",
    "registry:internal:build": "tsx --tsconfig ./tsconfig.scripts.json src/scripts/build-registry.mts",
    "registry:build": "pnpm registry:internal:build && shadcn build ./src/__registry__/registry.autogenerated.json",
    "capture": "tsx --tsconfig ./tsconfig.scripts.json src/scripts/capture.ts",
    "capture:components": "tsx --tsconfig ./tsconfig.scripts.json src/scripts/capture-components.ts",
    "generate-libphonenumber-metadata": "libphonenumber-metadata-generator ./src/assets/libphonenumber.metadata.json --countries FR --extended",
    "upgrade:next": "npx @next/codemod@latest upgrade latest",
    "upgrade:tailwind": "npx @tailwindcss/upgrade@latest",
    "prepare": "husky"
  },
